{
  "name": "copy-config",
  "version": "0.0.2",
  "description": "Copies another repo's configuration",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "scripts": {
    "copy-config": "node dist/bin --repo mmkal/eslint-plugin-codegen",
    "eslint": "eslint --ext '.ts,.js,.md'",
    "lint": "tsc && eslint .",
    "build": "tsc -p tsconfig.lib.json",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "arg": "^5.0.2",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "type-fest": "^3.5.0"
  },
  "devDependencies": {
    "@types/eslint": "8.37.0",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.192",
    "eslint": "8.37.0",
    "eslint-plugin-mmkal": "0.0.1-2",
    "fs-syncer": "0.4.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "typescript": "4.9.5",
    "np": "7.7.0"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "np": {
    "cleanup": false
  }
}
