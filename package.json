{
  "name": "copy-config",
  "version": "0.0.1-0",
  "description": "Copies another repo's configuration",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "scripts": {
    "copy-config": "node dist/bin --repo mmkal/eslint-plugin-codegen",
    "eslint": "eslint --ext '.ts,.js,.md'",
    "lint": "tsc && eslint .",
    "build": "tsc -p tsconfig.lib.json",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "arg": "^5.0.2",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "type-fest": "^3.5.0"
  },
  "devDependencies": {
    "@types/eslint": "7.2.6",
    "@types/glob": "8.0.0",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.14.191",
    "eslint": "8.23.0",
    "eslint-plugin-mmkal": "0.0.1-2",
    "fs-syncer": "0.4.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2",
    "np": "7.6.2"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "np": {
    "cleanup": false
  }
}
