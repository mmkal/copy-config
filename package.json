{
  "name": "copy-config",
  "version": "0.4.0-0",
  "description": "Copies another repo's configuration",
  "packageManager": "pnpm@8.10.2",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "homepage": "https://github.com/mmkal/copy-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmkal/copy-config.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "_dogfood": "node dist/bin --repo mmkal/eslint-plugin-codegen",
    "copy-config": "copy-config --repo mmkal/copy-config",
    "eslint": "eslint --ext '.ts,.js,.md'",
    "lint": "tsc && eslint .",
    "build": "rm -rf dist && tsc -p tsconfig.lib.json",
    "prepare": "pnpm build",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "mmkal",
  "license": "ISC",
  "dependencies": {
    "@rebundled/execa": "8.0.2-next.2",
    "arg": "^5.0.2",
    "execa": "^8.0.1",
    "glob": "^10.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "type-fest": "^3.5.0"
  },
  "devDependencies": {
    "@types/eslint": "8.40.0",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.195",
    "@types/node": "^20.11.17",
    "eslint": "^8.56.0",
    "eslint-plugin-mmkal": "0.3.0",
    "fs-syncer": "0.4.0",
    "np": "github:mmkal/np#pnpm",
    "typescript": "5.0.4",
    "vitest": "^1.2.2"
  },
  "files": [
    "dist",
    "*.md"
  ]
}
